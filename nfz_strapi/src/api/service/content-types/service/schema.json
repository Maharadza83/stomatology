      "type": "text",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "article": {
      "type": "blocks",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "popular",
        "treatment",
        "diagnostics"
      ]
    }
  }
}
